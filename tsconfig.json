{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021", "DOM"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["cypress", "node"],
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["cypress", "cypress.config.ts"]
}


